<header>
            <ul id="dropdown1" class="dropdown-content">
            <li><%= session_link %></li>
            <% if logged_in? %>
            <li><%= link_to "profile", student_path(current_student) %></li>
            <% end %>
            <li class="divider"></li>
          </ul>
          <ul id="dropdown2" class="dropdown-content">
            <li><%= session_link %></li>
            <% if logged_in? %>
            <li><%= link_to "profile", student_path(current_student) %></li>
            <% end %>
            <li class="divider"></li>
          </ul>


        <nav>
          <div class="container">
          <div class="nav-wrapper">
            <%= link_to "Punjab university", root_path, class: "brand-logo right" %>
            <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
            <ul>
                <li><%= link_to "My Courses", my_courses_students_path %></li>
            </ul>
            <ul id="nav-mobile" class="left hide-on-med-and-down">
              <li><a href="<%= root_path %>">Courses</a></li>
              <li><%= link_to "Students",students_path %></li>
              <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">Accounts<i class="material-icons right">arrow_drop_down</i></a></li>
            </ul>
              <ul class="sidenav" id="mobile-demo">
                <li><%= link_to "Courses", root_path %></li>
                <li><%= link_to "Students",students_path %></li>
                <li><a class="dropdown-trigger" href="#!" data-target="dropdown2">Accounts<i class="material-icons right">arrow_drop_down</i></a></li>
              </ul>
          </div>
        </div>
        </nav>
         <% flash.each do |key,message| %>
          <div class="container">
            <div class="row sign-up-form">
            <div class="col s12 m12 l12">
               <div class="card-panel red lighten-2" id="fade_out_target">
               <span class="white-text">
                <%= message %>
               </span>
           </div>
         </div>
       </div>
       </div>
      <% end %>
</header>